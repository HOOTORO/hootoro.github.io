{
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template",
                "goTemplateBracketSpacing": true,
                "bracketSameLine": true,
                "quoteProps": "consistent",
                "singleAttributePerLine": true
            }
        },
        {
            "files": ["*.js", "*.ts"],
            "options": {
                "useTabs": true,
                "printWidth": 120,
                "singleQuote": true
            }
        }
    ]
}
