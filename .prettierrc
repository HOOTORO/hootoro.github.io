{
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template",
                "plugins": ["prettier-plugin-go-template"]
            }
        },
        {
            "files": ["*.js", "*.ts"],
            "options": {
                "useTabs": true,
                "printWidth": 120,
                "singleQuote": true
            }
        }
    ]
}
